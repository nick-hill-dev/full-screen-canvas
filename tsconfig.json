{
  "noImplicitAny": false,
  "noImplicitReturns": true,
  "noUnusedLocals": false,
  "compilerOptions": {
    "module": "node16",
    "target": "es2022",
    "declaration": true,
    "sourceMap": true,
    "declarationMap": true,
    "outDir": "./dist",
    "strict": true,
    "rootDir": "./src",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": [
    "src/**/*"
  ]
}